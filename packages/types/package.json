{
  "name": "fmp-node-types",
  "version": "0.1.1",
  "description": "Shared TypeScript types for FMP Node Wrapper ecosystem (internal package)",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "npx eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "typescript",
    "types",
    "financial",
    "fmp",
    "financial-modeling-prep",
    "internal"
  ],
  "author": "Eric <https://github.com/e-roy>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/e-roy/fmp-node-wrapper.git"
  },
  "homepage": "https://fmp-node-wrapper-docs.vercel.app",
  "devDependencies": {
    "@types/node": "^20.11.0",
    "prettier": "^3.2.5",
    "tsup": "^8.0.0",
    "typescript": "^5.3.3"
  }
}
